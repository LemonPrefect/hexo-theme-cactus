<footer id="footer">
  <div class="footer-right">
    <nav>
      <ul>
        <% for (var i in theme.nav) { %> 
          <li><a href="<%- ((theme.nav[i] === '/archives/' || theme.nav[i] === '/') && page.lang != config.language[0])?('/' + page.lang + url_for(theme.nav[i])):url_for(theme.nav[i]) %>"><%= __('nav.'+i).replace("nav.", "") %></a></li>
        <% } %>
        <% const dictShow = {"zh-TW": "正", "zh-CN": "简", "en": "En"} %>
        <% const dictLabel = {"zh-TW": "正体中文（台灣標準）", "zh-CN": "中文简体（大陆标准）", "en": "English"} %>
        <li>
        <% Array.prototype.forEach.call(config.language, function(lang){ %>
          <a href="<%- (page.lang === lang) ? '#' : url_for('/' + lang) %>"><%- dictShow[lang] %></a>
        <% }) %>
        </li>
      </ul>
    </nav>
  </div>
  <div class="footer-left">
    <%= __('footer.copyright') %> &copy;
    <% var endYear = (theme.copyright && theme.copyright.end_year) ? theme.copyright.end_year : new Date().getFullYear() %>
    <% var startYear = (theme.copyright && theme.copyright.start_year) ? theme.copyright.start_year : new Date().getFullYear() %>
    <%= startYear >= endYear ? endYear : startYear + "-" + endYear %>
    <%= config.author || config.title %>
    <% if(theme.register.website_register != null){ %>
      <a href="http://beian.miit.gov.cn/"><%= theme.register.website_register %></a>
    <% } %>
    <% if(theme.register.cyber_register != null && theme.register.cyber_register_num != null){ %>
      <img src="/images/beian.png"/>
      <a href= "http://www.beian.gov.cn/portal/registerSystemInfo?recordcode=<%= theme.register.cyber_register_num %>"><%= theme.register.cyber_register %></a>
    <% } %>
    
  </div>
</footer>
